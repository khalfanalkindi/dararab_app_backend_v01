web: echo "SET FOREIGN_KEY_CHECKS = 0; DROP TABLE IF EXISTS django_content_type, auth_permission, auth_group, django_migrations, django_admin_log, django_session;" | mysql -h $MYSQLHOST -u $MYSQLUSER -p$MYSQLPASSWORD $MYSQLDATABASE && python manage.py migrate --noinput && python manage.py collectstatic --noinput && python manage.py createsuperuser --noinput && gunicorn backend.wsgi:application --bind 0.0.0.0:$PORT
